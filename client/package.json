    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "bootstrap": "^4.6.0",
    "date-fns": "^2.21.1",
    "formik": "^2.2.6",
    "i18next": "^20.2.1",
    "link-module-alias": "^1.2.0",
    "react": "^17.0.2",
    "react-bootstrap": "^1.5.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.8.13",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "storybook-addon-i18n": "^5.1.13",
    "styled-components": "^5.2.3",
